<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.genealogy_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.TreeActivity"></activity>
        <activity android:name=".Activities.RegisterActivity" />
        <activity android:name=".Activities.PersonalInfoActivity" />
        <activity android:name=".Activities.EditPersonActivity" />
        <activity android:name=".Activities.EditProfileActivity" />
        <activity android:name=".Activities.AddChildActivity" />
        <activity android:name=".Activities.AddSpouseActivity" />
        <activity android:name=".Activities.LoginActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>